<Window xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"  x:Class="SandBoxApp.Main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:Views="clr-namespace:SandBoxApp.Views"
        xmlns:local="clr-namespace:SandBoxApp"
        mc:Ignorable="d"
        Title="Main" Height="1000" Width="1400">
    <Grid>
        <dx:DXTabControl>
            <dx:DXTabItem Header="Main">
                <Views:MainView />
            </dx:DXTabItem>
            <dx:DXTabItem Header="Pagination">
                <Views:PaginiationView />
            </dx:DXTabItem>
            <dx:DXTabItem Header="Card Views">
                <Views:CardView />
            </dx:DXTabItem>
            <dx:DXTabItem Header="Object Views">
                <Views:ObjectView />
            </dx:DXTabItem>
            <dx:DXTabItem Header="Query Views">
                <Views:QueryView />
            </dx:DXTabItem>
            <dx:DXTabItem Header="Feed Views" IsSelected="True">
                <Views:FeedView />
            </dx:DXTabItem>

        </dx:DXTabControl>
    </Grid>
</Window>
